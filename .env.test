# http proxy perfix will effect when VITE_HTTP_PROXY is true.
VITE_HTTP_PROXY_PREFIX = ''

# if proxy is false, the requeset will link this
VITE_HTTP_API_URL = 'https://wallet.piccfu.com/picc-bussiness-center-gateway'

# usage visualizer ?
VITE_VISUALIZER = 'true'
